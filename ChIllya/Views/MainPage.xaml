<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChIllya.Views.MainPage"
             xmlns:contents="clr-namespace:ChIllya.Views.Contents"
             xmlns:components="clr-namespace:ChIllya.UIComponents"
             x:Name="main">
    <Grid   >
        <ContentView Content="{Binding MainContent, Source={x:Reference main}}"></ContentView>
    </Grid>
</ContentPage>